# AISET Environment Configuration
# DO-178C Traceability: REQ-CONFIG-001
# SECURITY: Never commit actual .env file with real credentials

# Database Configuration
DATABASE_URL=postgresql://aiset_user:password@localhost:5432/aiset_db
DB_ECHO=False

# AI Service Configuration
# Primary: Claude API
ANTHROPIC_API_KEY=sk-ant-your-key-here
ANTHROPIC_MODEL=claude-3-sonnet-20240229

# Fallback: LM Studio (Local)
LM_STUDIO_URL=http://localhost:1234/v1
LM_STUDIO_MODEL=mistral-7b-instruct

# AI Service Selection (claude or lmstudio)
AI_SERVICE=claude

# Security
SECRET_KEY=your-secret-key-here-min-32-characters
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Application Settings
APP_NAME=AISET
APP_VERSION=0.1.0
DEBUG=False
LOG_LEVEL=INFO

# CORS Settings
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000

# Export Settings
EXPORT_TEMPLATES_DIR=./templates
EXPORT_OUTPUT_DIR=./exports

# DO-178C Compliance Settings
ENABLE_AUDIT_TRAIL=True
REQUIRE_APPROVAL_WORKFLOW=True
TRACEABILITY_STRICT_MODE=True
