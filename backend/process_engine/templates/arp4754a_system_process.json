              "activity_id": "ACT_FUNC_INT_TEST",
              "name": "Functional Integration Testing",
              "type": "TEST",
              "required": true,
              "output_artifacts": ["FUNC_INT_TEST_RESULTS"]
            },
            {
              "activity_id": "ACT_INTERFACE_TEST",
              "name": "Interface Testing",
              "type": "TEST",
              "required": true,
              "output_artifacts": ["INTERFACE_TEST_RESULTS"]
            },
            {
              "activity_id": "ACT_RESOURCE_TEST",
              "name": "Resource Utilization Testing",
              "type": "TEST",
              "required": true,
