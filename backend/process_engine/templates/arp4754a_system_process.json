{
  "template_id": "ARP4754A_SYSTEM_V1",
  "name": "ARP4754A System Development Process",
  "description": "Complete system development lifecycle per ARP4754A Guidelines for Development of Civil Aircraft and Systems, aligned with Project_Plan.md 10-phase process",
  "standard": "ARP4754A",
  "version": "1.0.0",
  "applicable_ci_types": ["SYSTEM", "SUBSYSTEM"],
  "dal_levels": ["DAL_A", "DAL_B", "DAL_C", "DAL_D", "DAL_E"],

  "phases": [
    {
      "phase_id": "PH_REQUIREMENTS_CAPTURE",
      "name": "Phase 1: Requirements Capture & Definition",
      "description": "Capture stakeholder needs, develop system requirements, conduct initial safety assessment, plan architecture",
      "order": 1,
      "standard_reference": "ARP4754A Section 5.1",
      "entry_criteria": [
        {
          "criterion_id": "CR_PROJECT_CREATED",
          "description": "Project has been created in AISET",
          "check_type": "ARTIFACT_EXISTS",
          "check_config": {"artifact_type": "PROJECT"}
        }
      ],
      "exit_criteria": [
        {
          "criterion_id": "CR_STRS_COMPLETE",
          "description": "Stakeholder Requirements Specification complete",
          "check_type": "ARTIFACT_EXISTS",
          "check_config": {"artifact_type": "SPECIFICATION", "name_pattern": "StRS*"}
        },
        {
          "criterion_id": "CR_SYRS_COMPLETE",
          "description": "System Requirements Specification complete",
          "check_type": "ARTIFACT_EXISTS",
          "check_config": {"artifact_type": "SPECIFICATION", "name_pattern": "SyRS*"}
        },
        {
          "criterion_id": "CR_FHA_COMPLETE",
          "description": "Functional Hazard Assessment complete",
          "check_type": "ARTIFACT_EXISTS",
          "check_config": {"artifact_type": "SAFETY_ASSESSMENT", "name_pattern": "FHA*"},
          "dal_required": ["DAL_A", "DAL_B", "DAL_C", "DAL_D"]
        }
      ],
      "sub_phases": [
        {
          "sub_phase_id": "SP_STAKEHOLDER_NEEDS",
          "name": "Stakeholder Needs Capture",
          "description": "Capture functional, performance, safety, security, environmental, regulatory, and cost requirements from stakeholders",
          "order": 1,
          "activities": [
            {
              "activity_id": "ACT_CAPTURE_FUNCTIONAL",
              "name": "Capture Functional Requirements",
              "type": "INTERVIEW",
              "required": true,
              "interview_questions": [
                {"question_id": "Q_FUNC_001", "required": true},
                {"question_id": "Q_FUNC_002", "required": true}
              ],
              "output_artifacts": ["STAKEHOLDER_REQ_FUNCTIONAL"]
            },
            {
              "activity_id": "ACT_CAPTURE_PERFORMANCE",
              "name": "Capture Performance Requirements",
              "type": "INTERVIEW",
              "required": true,
              "output_artifacts": ["STAKEHOLDER_REQ_PERFORMANCE"]
            },
            {
              "activity_id": "ACT_CAPTURE_SAFETY",
              "name": "Capture Safety Requirements",
              "type": "INTERVIEW",
              "required": true,
              "output_artifacts": ["STAKEHOLDER_REQ_SAFETY"]
            },
            {
              "activity_id": "ACT_CAPTURE_SECURITY",
              "name": "Capture Security Requirements",
              "type": "INTERVIEW",
              "required": false,
              "output_artifacts": ["STAKEHOLDER_REQ_SECURITY"]
            },
            {
              "activity_id": "ACT_CAPTURE_ENV",
              "name": "Capture Environmental Requirements",
              "type": "INTERVIEW",
              "required": true,
              "output_artifacts": ["STAKEHOLDER_REQ_ENVIRONMENTAL"]
            },
            {
              "activity_id": "ACT_CAPTURE_REGULATORY",
              "name": "Capture Regulatory Requirements",
              "type": "INTERVIEW",
              "required": true,
              "output_artifacts": ["STAKEHOLDER_REQ_REGULATORY"]
            }
          ],
          "completion_criteria": [
            {
              "criterion_id": "CR_STAKEHOLDER_REQS",
              "description": "At least one stakeholder requirement captured",
              "check_type": "ALL_ITEMS_COMPLETE"
            }
          ]
        },
        {
          "sub_phase_id": "SP_SYSTEM_REQUIREMENTS",
          "name": "System Requirements Development",
          "description": "Derive technical system requirements from stakeholder needs",
          "order": 2,
          "activities": [
            {
              "activity_id": "ACT_DERIVE_SYSTEM_REQ",
              "name": "Derive System Requirements",
              "type": "INTERVIEW",
              "required": true,
              "input_artifacts": ["STAKEHOLDER_REQ_FUNCTIONAL", "STAKEHOLDER_REQ_PERFORMANCE"],
              "output_artifacts": ["SYSTEM_REQUIREMENTS"]
            },
            {
              "activity_id": "ACT_DEFINE_OPS_ENV",
              "name": "Define Operational & Environmental Conditions",
              "type": "INTERVIEW",
              "required": true,
              "output_artifacts": ["OPS_ENV_DEFINITION"]
            },
            {
              "activity_id": "ACT_ESTABLISH_DERIVED",
              "name": "Establish Derived Requirements",
              "type": "ANALYSIS",
              "required": true,
              "output_artifacts": ["DERIVED_REQUIREMENTS"]
            }
          ]
        },
        {
          "sub_phase_id": "SP_SAFETY_ASSESSMENT",
          "name": "Initial Safety & Risk Assessment",
          "description": "Conduct Functional Hazard Assessment per ARP4761",
          "order": 3,
          "activities": [
            {
              "activity_id": "ACT_FHA",
              "name": "Functional Hazard Assessment",
              "type": "ANALYSIS",
              "required": true,
              "dal_required": ["DAL_A", "DAL_B", "DAL_C", "DAL_D"],
              "output_artifacts": ["FHA_REPORT"]
            },
            {
              "activity_id": "ACT_HAZARD_ID",
              "name": "Identify Hazards and Failure Conditions",
              "type": "INTERVIEW",
              "required": true,
              "output_artifacts": ["HAZARD_LIST"]
            },
            {
              "activity_id": "ACT_SAFETY_OBJ",
              "name": "Establish Safety Objectives",
              "type": "ANALYSIS",
              "required": true,
              "output_artifacts": ["SAFETY_OBJECTIVES"]
            }
          ]
        },
        {
          "sub_phase_id": "SP_ARCH_PLANNING",
          "name": "Architecture Planning",
          "description": "Explore system architecture alternatives and conduct trade studies",
          "order": 4,
          "activities": [
            {
              "activity_id": "ACT_ARCH_ALTERNATIVES",
              "name": "Explore Architecture Alternatives",
              "type": "DESIGN",
              "required": true,
              "output_artifacts": ["ARCH_ALTERNATIVES"]
            },
            {
              "activity_id": "ACT_TRADE_STUDIES",
              "name": "Conduct Trade Studies",
              "type": "ANALYSIS",
              "required": true,
              "output_artifacts": ["TRADE_STUDY_REPORT"]
            },
            {
              "activity_id": "ACT_DEFINE_BOUNDARIES",
              "name": "Define System Boundaries and Interfaces",
              "type": "DESIGN",
              "required": true,
              "output_artifacts": ["SYSTEM_BOUNDARIES"]
            }
          ]
        }
      ],
      "deliverables": [
        {
          "deliverable_id": "DEL_STRS",
          "name": "Stakeholder Requirements Specification (StRS)",
          "artifact_type": "SPECIFICATION",
          "template_id": "TPL_STRS",
          "required": true
        },
        {
          "deliverable_id": "DEL_SYRS",
          "name": "System Requirements Specification (SyRS)",
          "artifact_type": "SPECIFICATION",
          "template_id": "TPL_SYRS",
          "required": true
        },
        {
          "deliverable_id": "DEL_OPS_ENV",
          "name": "Operational & Environmental Definition Document",
          "artifact_type": "SPECIFICATION",
          "template_id": "TPL_OPS_ENV",
          "required": true
        },
        {
          "deliverable_id": "DEL_FHA",
          "name": "Functional Hazard Assessment (FHA)",
          "artifact_type": "SAFETY_ASSESSMENT",
          "template_id": "TPL_FHA",
          "required": true,
          "dal_required": ["DAL_A", "DAL_B", "DAL_C", "DAL_D"]
        },
        {
          "deliverable_id": "DEL_SDP",
          "name": "System Development Plan (SDP)",
          "artifact_type": "PLAN",
          "template_id": "TPL_SDP",
          "required": true
        }
      ],
      "reviews": [
        {
          "review_id": "REV_SRR",
          "name": "System Requirements Review (SRR)",
          "type": "SRR",
          "standard_reference": "ARP4754A Section 5.4",
          "entry_artifacts": ["STAKEHOLDER_REQ", "SYSTEM_REQ", "FHA"],
          "exit_artifacts": ["SRR_RECORD"]
        }
      ],
      "parallel_allowed": false,
      "rollback_allowed": true
    },

    {
      "phase_id": "PH_ARCHITECTURE_DEFINITION",
      "name": "Phase 2: System Architecture Definition & Allocation",
      "description": "Define functional and physical architecture, allocate requirements, assign DAL levels",
      "order": 2,
      "standard_reference": "ARP4754A Section 5.2",
      "entry_criteria": [
        {
          "criterion_id": "CR_PHASE1_COMPLETE",
          "description": "Phase 1 requirements capture complete",
          "check_type": "ARTIFACT_APPROVED",
          "check_config": {"phase": "PH_REQUIREMENTS_CAPTURE"}
        }
      ],
      "exit_criteria": [
        {
          "criterion_id": "CR_ARCH_COMPLETE",
          "description": "System Architecture Description complete and approved",
          "check_type": "ARTIFACT_APPROVED",
          "check_config": {"artifact_type": "DESIGN_DOCUMENT", "name_pattern": "SAD*"}
        },
        {
          "criterion_id": "CR_DAL_ASSIGNED",
          "description": "DAL levels assigned to all subsystems",
          "check_type": "ALL_ITEMS_COMPLETE",
          "check_config": {"item_type": "DAL_ASSIGNMENT"}
        }
      ],
      "sub_phases": [
        {
          "sub_phase_id": "SP_FUNCTIONAL_ARCH",
          "name": "Functional Architecture Development",
          "description": "Define functions and data flows",
          "order": 1,
          "activities": [
            {
              "activity_id": "ACT_DEFINE_FUNCTIONS",
              "name": "Define System Functions",
              "type": "DESIGN",
              "required": true,
              "output_artifacts": ["FUNCTIONAL_ARCHITECTURE"]
            },
            {
              "activity_id": "ACT_DATA_FLOWS",
              "name": "Define Data Flows",
              "type": "DESIGN",
              "required": true,
              "output_artifacts": ["DATA_FLOW_DIAGRAMS"]
            }
          ]
        },
        {
          "sub_phase_id": "SP_PHYSICAL_ARCH",
          "name": "Physical Architecture Development",
          "description": "Define physical components, locations, redundancy",
          "order": 2,
          "activities": [
            {
              "activity_id": "ACT_PHYSICAL_COMPONENTS",
              "name": "Define Physical Components",
              "type": "DESIGN",
              "required": true,
              "output_artifacts": ["PHYSICAL_ARCHITECTURE"]
            },
            {
              "activity_id": "ACT_PARTITIONING",
              "name": "Establish Partitioning Strategy",
              "type": "DESIGN",
              "required": true,
              "dal_required": ["DAL_A", "DAL_B", "DAL_C"],
              "output_artifacts": ["PARTITIONING_STRATEGY"]
            }
          ]
        },
        {
          "sub_phase_id": "SP_REQ_ALLOCATION",
          "name": "Requirements Allocation",
          "description": "Allocate system requirements to subsystems and components",
          "order": 3,
          "activities": [
            {
              "activity_id": "ACT_ALLOCATE_REQS",
              "name": "Allocate Requirements to Subsystems",
              "type": "ANALYSIS",
              "required": true,
              "output_artifacts": ["ALLOCATION_MATRIX"]
            },
            {
              "activity_id": "ACT_INTERFACE_REQS",
              "name": "Establish Interface Requirements",
              "type": "DESIGN",
              "required": true,
              "output_artifacts": ["INTERFACE_REQUIREMENTS"]
            },
            {
              "activity_id": "ACT_DERIVED_SUBSYS",
              "name": "Identify Derived Requirements at Subsystem Level",
              "type": "ANALYSIS",
              "required": true,
              "output_artifacts": ["SUBSYSTEM_DERIVED_REQS"]
            }
          ]
        },
        {
          "sub_phase_id": "SP_SAFETY_ASSURANCE",
          "name": "Safety & Assurance",
          "description": "Update safety analysis and assign DAL levels",
          "order": 4,
          "activities": [
            {
              "activity_id": "ACT_PSSA",
              "name": "Preliminary System Safety Assessment",
              "type": "ANALYSIS",
              "required": true,
              "dal_required": ["DAL_A", "DAL_B", "DAL_C", "DAL_D"],
              "output_artifacts": ["PSSA_REPORT"]
            },
            {
              "activity_id": "ACT_DAL_ASSIGNMENT",
              "name": "Assign Development Assurance Levels",
              "type": "ANALYSIS",
              "required": true,
              "output_artifacts": ["DAL_ASSIGNMENTS"]
            },
            {
              "activity_id": "ACT_CCA_STRATEGY",
              "name": "Establish Common Cause Analysis Strategy",
              "type": "ANALYSIS",
              "required": true,
              "dal_required": ["DAL_A", "DAL_B"],
              "output_artifacts": ["CCA_STRATEGY"]
            }
          ]
        }
      ],
      "deliverables": [
        {
          "deliverable_id": "DEL_SAD",
          "name": "System Architecture Description",
          "artifact_type": "DESIGN_DOCUMENT",
          "required": true
        },
        {
          "deliverable_id": "DEL_ALLOC_MATRIX",
          "name": "Requirements Allocation Matrix",
          "artifact_type": "TRACEABILITY_MATRIX",
          "required": true
        },
        {
          "deliverable_id": "DEL_ICD",
          "name": "Interface Control Documents (ICDs)",
          "artifact_type": "SPECIFICATION",
          "required": true
        },
        {
          "deliverable_id": "DEL_PSSA",
          "name": "Preliminary System Safety Assessment (PSSA)",
          "artifact_type": "SAFETY_ASSESSMENT",
          "required": true,
          "dal_required": ["DAL_A", "DAL_B", "DAL_C", "DAL_D"]
        },
        {
          "deliverable_id": "DEL_SVP",
          "name": "System Verification Plan",
          "artifact_type": "PLAN",
          "required": true
        },
        {
          "deliverable_id": "DEL_SVALP",
          "name": "System Validation Plan",
          "artifact_type": "PLAN",
          "required": true
        }
      ],
      "reviews": [
        {
          "review_id": "REV_SDR",
          "name": "System Design Review (SDR)",
          "type": "SDR",
          "standard_reference": "ARP4754A Section 5.4",
          "entry_artifacts": ["SYSTEM_ARCHITECTURE", "ALLOCATION_MATRIX", "PSSA"],
          "exit_artifacts": ["SDR_RECORD"]
        }
      ],
      "parallel_allowed": false,
      "rollback_allowed": true
    },

    {
      "phase_id": "PH_DETAILED_DESIGN",
      "name": "Phase 3: Subsystem & Component Detailed Design",
      "description": "Detailed design per subsystem domain, implementation, subsystem verification",
      "order": 3,
      "standard_reference": "ARP4754A Section 5.3, DO-178C, DO-254",
      "entry_criteria": [
        {
          "criterion_id": "CR_ARCH_APPROVED",
          "description": "System architecture approved at SDR",
          "check_type": "REVIEW_PASSED",
          "check_config": {"review_id": "REV_SDR"}
        }
      ],
      "exit_criteria": [
        {
          "criterion_id": "CR_DESIGN_COMPLETE",
          "description": "All subsystem designs complete",
          "check_type": "ALL_ITEMS_COMPLETE"
        },
        {
          "criterion_id": "CR_UNIT_TESTS_PASS",
          "description": "Unit tests passed",
          "check_type": "COVERAGE_THRESHOLD",
          "check_config": {"threshold": 100}
        }
      ],
      "sub_phases": [
        {
          "sub_phase_id": "SP_SW_DESIGN",
          "name": "Software Detailed Design",
          "description": "Software design per DO-178C",
          "order": 1,
          "activities": [
            {
              "activity_id": "ACT_SW_HLD",
              "name": "Software High-Level Design",
              "type": "DESIGN",
              "required": true,
              "output_artifacts": ["SW_HLD"]
            },
            {
              "activity_id": "ACT_SW_LLD",
              "name": "Software Low-Level Design",
              "type": "DESIGN",
              "required": true,
              "dal_required": ["DAL_A", "DAL_B", "DAL_C"],
              "output_artifacts": ["SW_LLD"]
            },
            {
              "activity_id": "ACT_SW_CODE",
              "name": "Software Coding",
              "type": "IMPLEMENTATION",
              "required": true,
              "output_artifacts": ["SOURCE_CODE"]
            }
          ]
        },
        {
          "sub_phase_id": "SP_HW_DESIGN",
          "name": "Hardware Detailed Design",
          "description": "Hardware design per DO-254",
          "order": 2,
          "activities": [
            {
              "activity_id": "ACT_HW_DESIGN",
              "name": "Hardware Design",
              "type": "DESIGN",
              "required": true,
              "output_artifacts": ["HW_DESIGN", "SCHEMATICS"]
            },
            {
              "activity_id": "ACT_HW_IMPL",
              "name": "Hardware Implementation",
              "type": "IMPLEMENTATION",
              "required": true,
              "output_artifacts": ["HW_PROTOTYPE"]
            }
          ]
        },
        {
          "sub_phase_id": "SP_MECH_DESIGN",
          "name": "Mechanical Detailed Design",
          "description": "Mechanical design",
          "order": 3,
          "activities": [
            {
              "activity_id": "ACT_MECH_DESIGN",
              "name": "Mechanical Design",
              "type": "DESIGN",
              "required": true,
              "output_artifacts": ["CAD_MODELS", "DRAWINGS"]
            }
          ]
        },
        {
          "sub_phase_id": "SP_SUBSYS_VERIF",
          "name": "Subsystem Verification",
          "description": "Unit testing and component testing",
          "order": 4,
          "activities": [
            {
              "activity_id": "ACT_UNIT_TEST",
              "name": "Unit Testing",
              "type": "TEST",
              "required": true,
              "output_artifacts": ["UNIT_TEST_RESULTS"]
            },
            {
              "activity_id": "ACT_CODE_REVIEW",
              "name": "Code Reviews",
              "type": "DOCUMENT_REVIEW",
              "required": true,
              "dal_required": ["DAL_A", "DAL_B", "DAL_C"],
              "output_artifacts": ["CODE_REVIEW_RECORDS"]
            },
            {
              "activity_id": "ACT_DESIGN_REVIEW",
              "name": "Design Reviews",
              "type": "DOCUMENT_REVIEW",
              "required": true,
              "output_artifacts": ["DESIGN_REVIEW_RECORDS"]
            }
          ]
        },
        {
          "sub_phase_id": "SP_SUBSYS_SAFETY",
          "name": "Subsystem Safety Analysis",
          "description": "FTA, FMEA, CCA updates",
          "order": 5,
          "activities": [
            {
              "activity_id": "ACT_FTA",
              "name": "Fault Tree Analysis",
              "type": "ANALYSIS",
              "required": true,
              "dal_required": ["DAL_A", "DAL_B", "DAL_C"],
              "output_artifacts": ["FTA_REPORT"]
            },
            {
              "activity_id": "ACT_FMEA",
              "name": "Failure Mode Effects Analysis",
              "type": "ANALYSIS",
              "required": true,
              "dal_required": ["DAL_A", "DAL_B", "DAL_C", "DAL_D"],
              "output_artifacts": ["FMEA_REPORT"]
            }
          ]
        }
      ],
      "deliverables": [
        {
          "deliverable_id": "DEL_SUBSYS_DESIGN",
          "name": "Subsystem/Component Design Documents",
          "artifact_type": "DESIGN_DOCUMENT",
          "required": true
        },
        {
          "deliverable_id": "DEL_SOURCE_CODE",
          "name": "Source Code / Schematics / CAD Models",
          "artifact_type": "SOURCE_CODE",
          "required": true
        },
        {
          "deliverable_id": "DEL_UNIT_VERIF",
          "name": "Unit & Component Verification Reports",
          "artifact_type": "TEST_REPORT",
          "required": true
        },
        {
          "deliverable_id": "DEL_REQ_TRACE",
          "name": "Requirements Traceability Data",
          "artifact_type": "TRACEABILITY_MATRIX",
          "required": true
        },
        {
          "deliverable_id": "DEL_SAFETY_ANALYSES",
          "name": "Updated Safety Analyses (FMEA, FTA, CCA)",
          "artifact_type": "SAFETY_ASSESSMENT",
          "required": true,
          "dal_required": ["DAL_A", "DAL_B", "DAL_C", "DAL_D"]
        }
      ],
      "reviews": [
        {
          "review_id": "REV_PDR",
          "name": "Preliminary Design Review (PDR)",
          "type": "PDR",
          "entry_artifacts": ["SW_HLD", "HW_DESIGN"],
          "exit_artifacts": ["PDR_RECORD"]
        },
        {
          "review_id": "REV_CDR",
          "name": "Critical Design Review (CDR)",
          "type": "CDR",
          "entry_artifacts": ["SW_LLD", "DETAILED_DESIGN"],
          "exit_artifacts": ["CDR_RECORD"]
        }
      ],
      "parallel_allowed": true,
      "rollback_allowed": true
    },

    {
      "phase_id": "PH_INTEGRATION",
      "name": "Phase 4: System Integration & Qualification",
      "description": "Incremental integration, interface verification, integration testing",
