              "activity_id": "ACT_SSA",
              "name": "System Safety Assessment Update",
              "type": "ANALYSIS",
              "required": true,
              "dal_required": ["DAL_A", "DAL_B", "DAL_C", "DAL_D"],
              "output_artifacts": ["SSA_REPORT"]
            }
          ]
        }
      ],
      "deliverables": [
        {
          "deliverable_id": "DEL_INT_PROCEDURES",
          "name": "System Integration Procedures & Reports",
          "artifact_type": "TEST_PROCEDURE",
          "required": true
        },
        {
          "deliverable_id": "DEL_INT_RESULTS",
          "name": "Integration Test Results & Anomaly Reports",
          "artifact_type": "TEST_REPORT",
          "required": true
